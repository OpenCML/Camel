with <weight: float, bias: float>
func linear(x: float): float {
    return weight * x + bias
}

func activation(x: float): float {
    // return if x > 0.0 then 1.0 else 0.0
    // return if x > 0.0 then x else 0.0
    return x
}

with <weight: float, bias: float>
export func dense(x: float): float {
    return x->linear<weight, bias>->activation
}