func test_tensor_operations(): int sync {
    // 创建不同形状的tensor
    let a = ones([3, 3])
    let b = zeros([3, 3])
    let c = eye(3)
    
    // 基本运算测试
    let add_result = add(a, c)
    let sub_result = subtract(a, c)
    let mul_result = multiply(a, c)
    let div_result = divide(a, c)
    
    // 形状变换测试
    let flat = flatten(a)
    let trans = transpose(a)
    
    // 其他操作测试
    let d = diag(a)
    let s = sum(a)
    let m = mean(a)
    
    return 0
}

